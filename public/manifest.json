{ 
  "manifest_version": 2,
  "short_name": "treez extension",
  "name": "Treez",
  "description": "Learn about plants while you are browsing away!",
  "version": "0.0.4",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },

  "permissions": [
    "tabs"
  ],
  
  "icons": {
    "16": "logo16.png",
    "32": "logo32.png",
    "48": "logo48.png",
    "128": "logo128.png",
    "256": "logo256.png"
  }

}
